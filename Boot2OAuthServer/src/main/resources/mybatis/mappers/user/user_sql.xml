<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.test.app.user.mapper.OAuthUserDetailsMapper">

	<select id="selectUserById" resultType="kr.co.test.app.user.model.OAuthUserDetails" parameterType="string">
		SELECT *
		  FROM oauth_user_details
		 WHERE username = #{username}
	</select>

</mapper>